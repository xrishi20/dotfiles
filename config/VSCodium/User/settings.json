{
  "editor.fontWeight": "400",
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.lineHeight": 1.625,
  "terminal.integrated.fontFamily": "'JetBrainsMono NF', Consolas, 'Courier New', monospace",
  "terminal.integrated.fontWeight": 400,
  "editor.fontLigatures": true,
  "editor.wordWrap": "on",
  "diffEditor.wordWrap": "on",
  "git.autofetch": true,
  "editor.fontFamily": "'Dank Mono', 'JetBrainsMono NF', Consolas, 'Courier New', monospace",
  "editor.linkedEditing": true,
  "emmet.excludeLanguages": ["markdown"],
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "postcss": "css",
    "twig": "html"
  },
  "editor.snippetSuggestions": "top",
  "editor.inlayHints.fontFamily": "'Dank Mono', 'JetBrainsMono NF', Consolas, 'Courier New', monospace",
  "editor.cursorStyle": "line",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorBlinking": "smooth",
  "editor.smoothScrolling": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.options": {
    "env": {
      "browser": true,
      "jest/globals": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2019,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "no-debugger": "off"
    }
  },
  "files.defaultLanguage": "{activeEditorLanguage}",
  "editor.wordBasedSuggestions": "off",
  "editor.suggest.localityBonus": true,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.enabled": false,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "entity.name.type.class",
          "keyword",
          "constant",
          "storage.modifier",
          "storage.type.class.js",
          "entity.other.attribute-name",
          "entity.name.function"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "invalid",
          "keyword.operator",
          "constant.numeric.css",
          "keyword.other.unit.px.css",
          "constant.numeric.decimal.js",
          "constant.numeric.json"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell",
      "args": ["-ExecutionPolicy", "Bypass"]
    }
  },
  "eslint.format.enable": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.codeLensFontFamily": "'Dank Mono', 'JetBrainsMono NF', Consolas, 'Courier New', monospace",
  "markdown.preview.lineHeight": 1.5,
  "scm.inputFontSize": 16,
  "files.associations": {
    "*.tsx": "typescriptreact",
    "*.twig": "twig"
  },
  "prettier.tabWidth": 4,
  "prettier.useTabs": true,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.fontWeightBold": "500",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "git.confirmSync": false,
  "[html]": {
    "editor.defaultFormatter": "vscode.emmet"
  },
  "[twig]": {
    "editor.defaultFormatter": "mblode.twig-language-2"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "symbols.hidesExplorerArrows": false,
  "window.menuBarVisibility": "toggle",
  "editor.renderWhitespace": "none",
  "debug.terminal.clearBeforeReusing": true,
  "workbench.layoutControl.enabled": false,
  "window.titleBarStyle": "custom",
  "terminal.integrated.defaultProfile.linux": "fish",
  "editor.fontVariations": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "git.openRepositoryInParentFolders": "never",
  "window.dialogStyle": "custom",
  "editor.tabSize": 3,
  "prettier.requireConfig": true,
  "terminal.integrated.fontSize": 15,
  "workbench.startupEditor": "none",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.fontSize": 16,
  "workbench.iconTheme": "symbols",
}
